// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Todos {
  id          String @id @default(uuid())
  title       String @unique
  description String
  is_Complete  Boolean
}

model Notes {
  id          String @id @default(uuid())
  title       String @unique
  description String
}
